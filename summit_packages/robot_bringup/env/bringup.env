export ROBOT_BRINGUP_SIM_PACKAGE=summit_xl_advanced_sim

# String. Set the bringup launch file
export ROBOT_BRINGUP_LAUNCH=robot_complete.launch
# String. Set the simulation bringup launch file
export ROBOT_BRINGUP_SIM_LAUNCH=summit_xl_complete.launch
