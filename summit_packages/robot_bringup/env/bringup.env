export ROBOT_RUN_ROBOT_LOCAL_CONTROL=true

export ROBOT_RUN_LOCALIZATION=true

export ROBOT_RUN_PERCEPTION=true

export ROBOT_RUN_NAVIGATION=true

export ROBOT_RUN_HMI=true

export ROBOT_RUN_COMMAND_MANAGER=true

export ROBOT_RUN_SENSORS=true

export ROBOT_RUN_ARM=true

export ROBOT_RUN_NTRIP=true

export ROBOT_RUN_ROSBAG_MANAGER=true

export ROBOT_RUN_INTERFACES=true

export ROBOT_RUN_SAFETY=true

# String. System path to the default workspace
export ROBOT_WORKSPACE=/home/robot/catkin_ws
