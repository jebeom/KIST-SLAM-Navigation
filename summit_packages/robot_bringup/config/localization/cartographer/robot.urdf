  <gazebo>
    <plugin filename="libhector_gazebo_ros_gps.so" name="summit_xl_gps_controller">
      <alwaysOn>1</alwaysOn>
      <updateRate>5</updateRate>
      <bodyName>summit_xl_gps_base_link</bodyName>
      <!-- must be the link of the gps device, not the base_link or base_footprint -->
      <frameId>summit_xl_gps_base_link</frameId>
      <!-- which frame_id would be on the published message -->
      <topicName>gps/fix</topicName>
      <!-- GPS coordinates -->
      <referenceLatitude>0</referenceLatitude>
      <referenceLongitude>0</referenceLongitude>
      <!-- To set heading in ENU orientation (degrees) -->
      <referenceHeading>90</referenceHeading>
      <velocityTopicName>gps/fix_velocity</velocityTopicName>
      <drift>0.0 0.0 0.0</drift>
      <!--<drift>0.0001 0.0001 0.0001</drift>-->
      <!--<drift>0.3 0.3 0.3</drift>-->
      <gaussianNoise>0.00001 0.00001 0.00001</gaussianNoise>
      <!--<gaussianNoise>0.00001 0.00001 0.00001</gaussianNoise>-->
      <velocityDrift>0.00001 0.00001 0.00001</velocityDrift>
      <!--<velocityGaussianNoise>0.1 0.1 0.1</velocityGaussianNoise>-->
      <velocityGaussianNoise>0.00001 0.00001 0.00001</velocityGaussianNoise>
    </plugin>
  </gazebo>
  <joint name="summit_xl_top_3d_laser_base_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.11808 0 0.38909"/>
    <parent link="summit_xl_base_link"/>
    <child link="summit_xl_top_3d_laser_base_link"/>
  </joint>
  <link name="summit_xl_top_3d_laser_base_link">
    <visual>
      <!-- origin xyz="0 0 0" rpy="0 0 1.5708"/ -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://robotnik_sensors/meshes/rs_helios_16p.stl"/>
      </geometry>
      <material name="black_alu"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://robotnik_sensors/meshes/rs_helios_16p.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.840"/>
      <origin xyz="0 0 0.04135"/>
      <inertia ixx="0.0013104203" ixy="0" ixz="0" iyy="0.0013104203" iyz="0" izz="0.00166334"/>
    </inertial>
  </link>
  <joint name="summit_xl_top_3d_laser_joint" type="fixed">
    <parent link="summit_xl_top_3d_laser_base_link"/>
    <child link="summit_xl_top_3d_laser_link"/>
    <origin rpy="0 0 0" xyz="0.0 0 0.0635"/>
  </joint>
  <link name="summit_xl_top_3d_laser_link"/>
  <gazebo reference="summit_xl_top_3d_laser_link">
    <sensor name="summit_xl_top_3d_laser_sensor" type="ray">
      <pose>0 0 0 0 0 0.0</pose>
      <visualize>false</visualize>
      <update_rate>20.0</update_rate>
      <ray>
        <scan>
          <horizontal>
            <!-- for a high quality simulation -->
            <samples>1800.0</samples>
            <min_angle>-3.14159265359</min_angle>
            <max_angle>3.14159265359</max_angle>
          </horizontal>
          <vertical>
            <samples>64.0</samples>
            <min_angle>-0.279252680319</min_angle>
            <max_angle>0.279252680319</max_angle>
          </vertical>
        </scan>
        <range>
          <min>0.2</min>
          <max>150.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>none</type>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_velodyne_laser.so" name="summit_xl_top_3d_laser_controller">
        <topicName>top_3d_laser/points</topicName>
        <frameName>/summit_xl_top_3d_laser_link</frameName>
        <gaussianNoise>0.008</gaussianNoise>
      </plugin>
    </sensor>
  </gazebo>
</robot>
